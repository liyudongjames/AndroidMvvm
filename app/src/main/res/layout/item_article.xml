<?xml version="1.0" encoding="utf-8"?><layout xmlns:android="http://schemas.android.com/apk/res/android">    <data>        <variable            name="articlebean"            type="com.example.liyudong.mvvm.net.bean.PublicArticalListBean" />    </data>    <RelativeLayout        xmlns:tools="http://schemas.android.com/tools"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="vertical"        android:padding="20dp">        <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/tv_name"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:textColor="#ff2262"            android:textSize="15sp"            android:text="@{articlebean.name}"            tools:text="123123131" />        <androidx.appcompat.widget.AppCompatTextView            android:id="@+id/tv_id"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentRight="true"            android:textColor="#ff2262"            android:textSize="15sp"            android:text="@{articlebean.id}"            tools:text="123123131" />    </RelativeLayout></layout>